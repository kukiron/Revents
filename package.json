{
  "name": "revents",
  "version": "1.0.0",
  "description": "Event organizing app Revents, built with React, Redux, Semantic-UI & Firestore",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf ./build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js, .jsx",
    "lint:fix": "eslint --fix . --ext .js, .jsx",
    "deploy": "npm run build && firebase deploy",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "keywords": [
    "React",
    "Redux",
    "Redux-Router",
    "Semantic-UI-React",
    "Firebase/Firestore"
  ],
  "author": "Kafil Uddin",
  "license": "ISC",
  "dependencies": {
    "cuid": "^2.1.1",
    "date-fns": "^1.29.0",
    "firebase": "4.12.1",
    "google-map-react": "^1.0.3",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-cropper": "^1.0.1",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.10",
    "react-infinite-scroller": "^1.2.0",
    "react-lazyload": "^2.3.0",
    "react-load-script": "0.0.6",
    "react-loadable": "^5.4.0",
    "react-places-autocomplete": "^6.1.3",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.0.6",
    "react-redux-toastr": "^7.2.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.3.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "revalidate": "^1.2.0",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.80.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "prettier": "^1.12.1",
    "redux-devtools-extension": "^2.13.2",
    "source-map-explorer": "^1.5.0"
  }
}
